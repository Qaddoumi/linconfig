{
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    }, // zooms out by 20%
    "search.searchEditor.doubleClickBehaviour": "goToLocation",
    "search.searchOnType": true,
    "search.searchOnTypeDebouncePeriod": 300,
    "search.showLineNumbers": true,
    "markdown.validate.enabled": true,
    "editor.renderWhitespace": "boundary",
    "editor.suggestSelection": "first",
    "editor.colorDecorators": true,
    "window.newWindowDimensions": "maximized",
    "indentRainbow.colorOnWhiteSpaceOnly": false,
    "java.semanticHighlighting.enabled": true,
    "html-css-class-completion.HTMLLanguages": [
        "html",
        "vue",
        "razor",
        "blade",
        "handlebars",
        "twig",
        "django-html",
        "php",
        "markdown",
        "erb",
        "ejs",
        "svelte"
    ],
    "html-css-class-completion.CSSLanguages": [
        "css",
        "sass",
        "scss"
    ],
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": "on"
    },
    "editor.quickSuggestionsDelay": 10,
    "workbench.remoteIndicator.showExtensionRecommendations": true,
    "editor.tabCompletion": "on",
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true
    },
    "editor.formatOnType": false,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.guides.indentation": true,
    "telemetry.telemetryLevel": "off",
    // "workbench.colorCustomizations": {
    //     "editorLineNumber.foreground": "#00ff00"
    // },
    "editor.acceptSuggestionOnEnter": "off",
    "files.associations": {
        "*.html": "html",
        "*.py": "python"
    },
    "gitlens.defaultDateFormat": null,
    "gitlens.hovers.currentLine.over": "line",
    "debug.console.fontFamily": "JetBrainsMono Nerd Font Propo",
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Propo",
    "terminal.integrated.fontSize": 11,
    "files.exclude": {
        "**/.git": false,
        "// **/.git": true
    },
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "[markdown]": {
        "editor.unicodeHighlight.ambiguousCharacters": true,
        "editor.unicodeHighlight.invisibleCharacters": true,
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.fontFamily": "JetBrainsMono Nerd Font Propo",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "powershell.integratedConsole.showOnStartup": true,
    "terminal.integrated.hideOnStartup": "always",
    "powershell.enableProfileLoading": true,
    "javascript.referencesCodeLens.showOnAllFunctions": true,
    "settingsSync.ignoredSettings": [],
    "settingsSync.ignoredExtensions": [],
    "chat.editor.fontFamily": "JetBrainsMono Nerd Font Propo",
    "workbench.settings.applyToAllProfiles": [],
    "window.systemColorTheme": "dark",
    "terminal.integrated.gpuAcceleration": "on",
    "redhat.telemetry.enabled": false,
    "editor.fontSize": 13,
    "debug.console.fontSize": 12,
    "scm.inputFontSize": 11,
    "chat.editor.fontSize": 12,
    "extensions.closeExtensionDetailsOnViewChange": false,
    "editor.linkedEditing": true,
    "typescript.suggest.paths": true,
    "javascript.suggest.paths": true,
    "markdown.suggest.paths.enabled": true,
    "files.hotExit": "onExit",
    "workbench.colorTheme": "Night Owl",
    "powershell.scriptAnalysis.settingsPath": "%USERPROFILE%\\Documents\\PowerShell\\PSScriptAnalyzerSettings.psd1",
    "database-client.autoSync": true,
    "editor.inlineSuggest.fontFamily": "JetBrainsMono Nerd Font Propo",
    "scm.inputFontFamily": "JetBrainsMono Nerd Font Propo",
    "notebook.markup.fontFamily": "JetBrainsMono Nerd Font Propo",
    "notebook.output.fontFamily": "JetBrainsMono Nerd Font Propo",
    "gitlens.currentLine.fontFamily": "JetBrainsMono Nerd Font Propo",
    "gitlens.blame.fontFamily": "JetBrainsMono Nerd Font Propo",
    "editor.codeLensFontFamily": "JetBrainsMono Nerd Font Propo",
    "editor.inlayHints.fontFamily": "JetBrainsMono Nerd Font Propo",
    "terminal.integrated.fontLigatures.enabled": true,
    "github.copilot.enable": {
        "markdown": true,
        "scminput": true
    },
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4.1",
    "powershell.promptToUpdatePowerShell": false,
    "qt-core.additionalQtPaths": [
        {
            "name": "Qt-5.15.18-linux-g++_from_PATH",
            "path": "/usr/bin/qmake"
        }
    ],
    "qt-qml.doNotAskForQmllsDownload": true,
    "amp.url": "https://ampcode.com/",
    "editor.wordBasedSuggestions": "off",
    "amp.tab.enabled": false,
    "workbench.sideBar.location": "right",
    "editor.semanticHighlighting.enabled": false,
}